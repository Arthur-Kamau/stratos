{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Stratos",
	"patterns": [
		{
			"match": "^(#!.*)$",
			"name": "meta.preprocessor.script.dart"
		},
		{
			"begin": "^\\w*\\b(library|use|import|part of|part|export)\\b",
			"beginCaptures": {
				"0": {
					"name": "keyword.other.import.dart"
				}
			},
			"end": ";",
			"endCaptures": {
				"0": {
					"name": "punctuation.terminator.dart"
				}
			},
			"name": "meta.declaration.dart",
			"patterns": [
				{
					"include": "#strings"
				},
				{
					"include": "#comments"
				},
				{
					"match": "\\b(as|show|hide)\\b",
					"name": "keyword.other.import.dart"
				}
			]
		},
		{
			"include": "#keywords"
		},
		{
			"include": "#comments"
		},
		{
			"include": "#punctuation"
		},
		{
			"include": "#annotations"
		},
		{
			"include": "#operators"
		},
		{
			"include": "#constants-and-special-vars"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#string-empty"
		},
		{
			"include": "#string-multiline"
		},
		{
			"include": "#character"
		},
		{
			"include": "#function-declaration"
		},
		{
			"include": "#null-literal"
		}
	],
	"fileTypes": [
		"st",
		"sts"
	],
	"repository": {
		"constants-and-special-vars": {
			"patterns": [
				{
					"match": "(?<!\\$)\\b(true|false|null)\\b(?!\\$)",
					"name": "constant.language.stratos"
				},
				{
					"match": "(?<!\\$)\\b(this|super)\\b(?!\\$)",
					"name": "variable.language.stratos"
				},
				{
					"match": "(?<!\\$)\\b((0(x|X)[0-9a-fA-F]*)|(([0-9]+\\.?[0-9]*)|(\\.[0-9]+))((e|E)(\\+|-)?[0-9]+)?)\\b(?!\\$)",
					"name": "constant.numeric.stratos"
				},
				{
					"match": "(?<![a-zA-Z0-9_$])([_$]*[A-Z][a-zA-Z0-9_$]*|bool\\b|num\\b|int\\b|double\\b|dynamic\\b)",
					"name": "support.class.stratos"
				},
				{
					"match": "(?<!\\$)\\b(sync(\\*)?|async(\\*)?|await|yield(\\*)?)\\b(?!\\$)",
					"name": "keyword.control.future.stratos"
				},
				{
					"match": "(?<!\\$)\\b(abstract|namespace|class|enum|extends|extension|external|factory|implements|get|mixin|native|operator|set|typedef|with|covariant)\\b(?!\\$)",
					"name": "keyword.declaration.stratos"
				},
				{
					"match": "(?<!\\$)\\b(is\\!?)\\b(?!\\$)",
					"name": "keyword.operator.stratos"
				},
				{
					"match": "\\?|:",
					"name": "keyword.operator.ternary.stratos"
				},
				{
					"match": "(<<|>>>?|~|\\^|\\||&)",
					"name": "keyword.operator.bitwise.stratos"
				},
				{
					"match": "((&|\\^|\\||<<|>>>?)=)",
					"name": "keyword.operator.assignment.bitwise.stratos"
				},
				{
					"match": "(=>)",
					"name": "keyword.operator.closure.stratos"
				},
				{
					"match": "(==|!=|<=?|>=?)",
					"name": "keyword.operator.comparison.stratos"
				},
				{
					"match": "(([+*/%-]|\\~)=)",
					"name": "keyword.operator.assignment.arithmetic.stratos"
				},
				{
					"match": "(=)",
					"name": "keyword.operator.assignment.stratos"
				},
				{
					"match": "(\\-\\-|\\+\\+)",
					"name": "keyword.operator.increment-decrement.stratos"
				},
				{
					"match": "(\\-|\\+|\\*|\\/|\\~\\/|%)",
					"name": "keyword.operator.arithmetic.stratos"
				},
				{
					"match": "(!|&&|\\|\\|)",
					"name": "keyword.operator.logical.stratos"
				},
				{
					"match": "(?<!\\$)\\b(static|final|const|required|late)\\b(?!\\$)",
					"name": "storage.modifier.stratos"
				},
				{
					"match": "(?<!\\$)\\b(?:void|var|let)\\b(?!\\$)",
					"name": "storage.type.primitive.stratos"
				},
				{
					"match": "([_$]*[a-z][a-zA-Z0-9_$]*)(<|\\(|\\s+=>)",
					"captures": {
						"1": {
							"name": "entity.name.function.stratos"
						}
					}
				}
			]
		},
		"comments": {
			"patterns": [
				{
					"include": "#comment-line"
				},
				{
					"include": "#comment-block"
				},
				{
					"include": "#comment-javadoc"
				}
			]
		},
		"comment-line": {
			"begin": "//",
			"end": "$",
			"name": "comment.line.double-slash.stratos"
		},
		"comment-block": {
			"begin": "/\\*(?!\\*)",
			"end": "\\*/",
			"name": "comment.block.stratos"
		},
		"comment-javadoc": {
			"patterns": [
				{
					"begin": "/\\*\\*",
					"end": "\\*/",
					"name": "comment.block.javadoc.stratos",
					"patterns": [
						{
							"match": "@(author|deprecated|return|see|serial|since|version)\\b",
							"name": "keyword.other.documentation.javadoc.stratos"
						},
						{
							"match": "(@param)\\s+(\\S+)",
							"captures": {
								"1": {
									"name": "keyword.other.documentation.javadoc.stratos"
								},
								"2": {
									"name": "variable.parameter.stratos"
								}
							}
						},
						{
							"match": "(@(?:exception|throws))\\s+(\\S+)",
							"captures": {
								"1": {
									"name": "keyword.other.documentation.javadoc.stratos"
								},
								"2": {
									"name": "entity.name.type.class.stratos"
								}
							}
						},
						{
							"match": "{(@link)\\s+(\\S+)?#([\\w$]+\\s*\\([^\\(\\)]*\\)).*}",
							"captures": {
								"1": {
									"name": "keyword.other.documentation.javadoc.stratos"
								},
								"2": {
									"name": "entity.name.type.class.stratos"
								},
								"3": {
									"name": "variable.parameter.stratos"
								}
							}
						}
					]
				}
			]
		},
		"annotations": {
			"patterns": [
				{
					"match": "@[a-zA-Z]+",
					"name": "storage.type.annotation.stratos"
				}
			]
		},
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control.stratos",
					"match": "\\b(if|while|for|return|end|begin|switch|break|unless|less|is|in|or|else|and|when|as)\\b"
				}
			]
		},
		"function-declaration": {
			"match": "\\b(fun)\\b\\s*(?<GROUP><([^<>]|\\g<GROUP>)+>)?\\s*(?:(\\w+)\\.)?(\\b\\w+\\b|`[^`]+`)",
			"captures": {
				"1": {
					"name": "storage.type.function.stratos"
				},
				"2": {
					"patterns": [
						{
							"include": "#type-parameter"
						}
					]
				},
				"4": {
					"name": "entity.name.type.class.extension.stratos"
				},
				"5": {
					"name": "entity.name.function.declaration.stratos"
				}
			}
		},
		"variable-declaration": {
			"match": "\\b(val|var)\\b\\s*(?<GROUP><([^<>]|\\g<GROUP>)+>)?",
			"captures": {
				"1": {
					"name": "storage.type.variable.stratos"
				},
				"2": {
					"patterns": [
						{
							"include": "#type-parameter"
						}
					]
				}
			}
		},
		"punctuation": {
			"patterns": [
				{
					"match": ",",
					"name": "punctuation.comma.stratos"
				},
				{
					"match": ";",
					"name": "punctuation.terminator.stratos"
				},
				{
					"match": "\\.",
					"name": "punctuation.dot.stratos"
				}
			]
		},
		"character": {
			"begin": "'",
			"end": "'",
			"name": "string.quoted.single.stratos",
			"patterns": [
				{
					"match": "\\\\.",
					"name": "constant.character.escape.stratos"
				}
			]
		},
		"strings": {
			"name": "string.quoted.double.stratos",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.stratos",
					"match": "\\\\."
				}
			]
		},
		"string-multiline": {
			"begin": "\"\"\"",
			"end": "\"\"\"",
			"name": "string.quoted.double.stratos",
			"patterns": [
				{
					"match": "\\\\.",
					"name": "constant.character.escape.stratos"
				},
				{
					"include": "#string-escape-simple"
				},
				{
					"include": "#string-escape-bracketed"
				}
			]
		},
		"string-empty": {
			"match": "(?<!\")\"\"(?!\")",
			"name": "string.quoted.double.stratos"
		},
		"operators": {
			"match": "<=|>=|===|==|=>|=|\\!==|\\!=|\\+=|\\+\\+|\\+|-=|--|-|\\*=|\\*|/=|/|%=|%|!|\\&\\&|\\&|\\|\\||\\|..",
			"name": "keyword.operator.stratos"
		},
		"null-literal": {
			"match": "\\bnill\\b",
			"name": "constant.language.null.stratos"
		},
		"string-escape-simple": {
			"match": "(?<!\\\\)\\$\\w+\\b",
			"name": "variable.string-escape.stratos"
		}
	},
	"scopeName": "source.stratos"
}