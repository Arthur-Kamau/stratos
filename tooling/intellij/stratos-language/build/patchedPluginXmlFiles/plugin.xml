<idea-plugin>
  <change-notes>
      Version 0.1.0 Notes.&lt;br&gt;
      &lt;em&gt;Initial Release&lt;/em&gt;</change-notes>
  <idea-version since-build="201.7223" until-build="201.*"/>
  <id>org.araizen.stratos</id>
  <name>Stratos Language</name>
  <version>1.0-SNAPSHOT</version>
  <vendor email="support@araizen.com" url="http://www.araizen.com">Araizen Technologies</vendor>
  <description>
    Stratos Language Pluguin .&lt;br&gt;
    &lt;em&gt;Support For Stratos File&lt;/em&gt;&lt;br&gt;
    &lt;em&gt;Autocompletion&lt;/em&gt;&lt;br&gt;
    &lt;em&gt;Code Navigation&lt;/em&gt;&lt;br&gt;
    &lt;em&gt;Syntax highlighting&lt;/em&gt;&lt;br&gt;
    &lt;em&gt;Code debugger &lt;/em&gt;&lt;br&gt;
    </description>
  <depends>com.intellij.modules.platform</depends>
  <depends>com.intellij.modules.lang</depends>
  <extensions defaultExtensionNs="com.intellij">
    <fileType name="Stratos File" implementationClass="com.araizen.stratos.files.StratosFileType" fieldName="INSTANCE" language="Stratos" extensions="st"/>
    <fileTypeFactory implementation="com.araizen.stratos.files.StratosFileTypeFactory"/>
  </extensions>
  <actions>
    <group id="RunAction" text="Stratos" description="Building and running stratos code">
      <add-to-group group-id="MainMenu" anchor="last"/>
      <action class="com.araizen.stratos.actions.RunAction" id="com.araizen.stratos.app.RunAction" text="Run App"/>
      <action class="com.araizen.stratos.actions.BuildAction" id="com.araizen.stratos.app.BuildAction" text="Build Executable"/>
      <action class="com.araizen.stratos.actions.FeedBackAction" id="com.araizen.stratos.app.FeedBackAction" text="Feedback"/>
      <action class="com.araizen.stratos.actions.DocumentationAction" id="com.araizen.stratos.app.DocumentationAction" text="Documentation"/>
    </group>
  </actions>
</idea-plugin>
