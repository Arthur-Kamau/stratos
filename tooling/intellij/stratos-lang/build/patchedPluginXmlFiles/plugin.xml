<idea-plugin>
  <change-notes>
      Add change notes here.&lt;br&gt;
      &lt;em&gt;most HTML tags may be used&lt;/em&gt;</change-notes>
  <idea-version since-build="202.6397" until-build="202.*"/>
  <id>stratos</id>
  <name>Stratos Language</name>
  <version>1.0-SNAPSHOT</version>
  <vendor email="kamaukenn11@gmail.com.com" url="https://github.com/Arthur-Kamau/stratos">Stratos</vendor>
  <description>
   Stratos Language Plugin.&lt;br&gt;
    &lt;em&gt;Project creating&lt;/em&gt;
    &lt;em&gt;Syntax highlighting&lt;/em&gt;
    &lt;em&gt;Commenting code&lt;/em&gt;
    </description>
  <depends>com.intellij.modules.platform</depends>
  <depends>com.intellij.modules.lang</depends>
  <extensions defaultExtensionNs="com.intellij">
    <fileType name="Stratos File" implementationClass="lang.stratos.language.StratosFileType" fieldName="INSTANCE" language="stratos" extensions="st"/>
    <lang.commenter language="stratos" implementationClass="lang.stratos.commentor.StratosCommenter"/>
    <lang.syntaxHighlighterFactory language="stratos" implementationClass="lang.stratos.language.StratosSyntaxHighlighterFactory"/>
  </extensions>
  <actions>
    <action id="Stratos.NewFile" class="lang.stratos.action.NewStratosFileAction" text="Stratos File" description="Create new Stratos file">
      <add-to-group group-id="NewGroup" anchor="before" relative-to-action="NewFromTemplate"/>
    </action>
  </actions>
</idea-plugin>
